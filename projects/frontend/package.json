		"commitizen": "^4.3.1",
		"cross-env": "^7.0.3",
		"cz-git": "^1.11.1",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.6",
		"pkg-types": "^1.3.1",
		"prettier": "^3.5.3",
		"prettier-plugin-lint-md": "^1.0.1",
		"rimraf": "^6.0.1",
		"tsx": "^4.19.3",
		"turbo": "^2.5.0",
		"type-plus": "^7.6.2",
		"typescript": "^5.8.3",
		"typescript-lodash": "^0.2.2",
		"vercel": "39.3.0",
		"vite": "^6.2.5",
		"vue": "^3.5.13"

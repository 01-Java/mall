	"packageManager": "pnpm@9.15.4",
	"engines": {
		"vscode": "^1.22.0",
		"node": ">=20.15.1",
		"pnpm": ">=9.5.0"
	},
	"keywords": [],
	"scripts": {
		"_preinstall_no_use": "npm run corepack:pnpm && npx only-allow pnpm",
		"corepack:pnpm": "corepack install",
		"git:fetch": "git fetch -p",
		"up:ruan-cat": "pnpm -F=* up @ruan-cat/*@latest",
		"update-package": "pnpm dlx vp-update",
		"update-package:all": "pnpm -r update-package && pnpm run update-package",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prettier": "prettier --write .",
		"test:vitest": "vitest --ui --watch",
		"rm:node_modules": "pnpm -r rm:node_modules && rimraf node_modules pnpm-lock.yaml",
		"rm:node_modules-2": "rimraf node_modules pnpm-lock.yaml && rimraf */**/node_modules",
		"code-workspace:create": "node --import=tsx ./scripts/generate.code-workspace.ts --name=01星球-商城项目"
	},
	"dependencies": {
		"lodash-es": "^4.17.21",
		"pinia-plugin-persistedstate": "^4.2.0"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^4.11.0",
		"@ruan-cat/commitlint-config": "^1.0.5",
		"@ruan-cat/generate-code-workspace": "^0.3.18",

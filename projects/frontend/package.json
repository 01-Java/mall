		"pkg-types": "^1.3.1",
		"prettier": "^3.6.2",
		"prettier-plugin-lint-md": "^1.0.1",
		"rimraf": "^6.0.1",
		"taze": "^19.1.0",
		"tsx": "^4.20.4",
		"turbo": "^2.5.6",
		"type-plus": "^7.6.2",
		"typescript": "^5.9.2",
		"typescript-lodash": "^0.2.2",
		"vercel": "39.3.0",
